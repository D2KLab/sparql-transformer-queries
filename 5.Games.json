{
  "proto": {
    "id": "?game",
    "title": "$foaf:name$required$var:title"
  },
  "$where": "?game a dbo:Game",
  "$orderby": "?title",
  "$prefixes": {
    "dbo" : "http://dbpedia.org/ontology/",
    "foaf":"http://xmlns.com/foaf/0.1/",
    "rdf" : "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  },
  "grlc":   {
    "summary":  "Games",
    "endpoint": "http://dbpedia.org/sparql",
    "tags": ["games"],
    "method": "GET",
    "pagination": 50
    }
}
